{
  "name": "new-marketing-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "npm run gen",
    "postinstall": "npm run gen",
    "build": "next build",
    "lint": "next lint",
    "start": "next start",
    "start:dev": "next dev",
    "gen": "npm run gen:graphql && npm run gen:chakraui",
    "gen:graphql": "cross-env DOTENV_CONFIG_PATH=.env graphql-codegen -r dotenv/config --config codegen.yml",
    "gen:chakraui": "chakra-cli tokens ./src/utilities/theme"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@graphcms/rich-text-react-renderer": "^0.1.5",
    "@graphql-codegen/import-types-preset": "^1.18.6",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.10",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.8.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.1",
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-hook": "^4.0.3",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.9.0",
    "@graphcms/rich-text-types": "^0.1.4",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@graphql-codegen/typescript-resolvers": "1.20.0",
    "@types/react": "17.0.15",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "typescript": "4.3.5"
  }
}
